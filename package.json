{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --exclude=**/e2e/** --exclude=**/integration/** --exclude=**/performance/**",
    "test:integration": "vitest run src/test/integration/**/*.test.{ts,tsx}",
    "test:e2e": "vitest run src/test/e2e/**/*.test.{ts,tsx}",
    "test:performance": "vitest run src/test/performance/**/*.test.{ts,tsx}",
    "test:watch": "vitest",
    "test:watch:unit": "vitest src/**/*.test.{ts,tsx} --exclude=src/test/{e2e,integration,performance}/**",
    "test:changed": "vitest related --run",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results/junit.xml",
    "test:debug": "vitest --inspect-brk --no-coverage",
    "test:comprehensive": "npx tsx src/test/scripts/runAllTests.ts",
    "test:metrics": "npx tsx src/test/utils/testMetrics.ts",
    "test:seed": "npx tsx src/test/utils/testSeeder.ts",
    "check-db": "node check_database_schema.js",
    "check-db:verbose": "node check_database_schema.js --verbose",
    "setup-stock": "node scripts/seed-existing-stock-table.js",
    "check-stock": "node scripts/check-and-fix-stock-schema.js",
    "seed:warehouse": "node scripts/seed-warehouse-data.js",
    "seed:simple": "node scripts/simple-seed.js",
    "migrate": "node run_migration.js",
    "create-tables": "node create_tables_directly.js",
    "deploy:setup": "node scripts/setup-environment.js",
    "deploy:build": "node scripts/build-production.js",
    "deploy:migrate": "node scripts/migrate-production.js",
    "backup:create": "node scripts/backup-database.js",
    "backup:restore": "node scripts/restore-database.js",
    "backup:setup-cron": "node scripts/setup-cron-backup.js",
    "deploy:netlify": "node scripts/deploy-netlify.js --preview",
    "deploy:netlify:prod": "node scripts/deploy-netlify.js --prod",
    "deploy:full": "npm run deploy:build && npm run deploy:netlify:prod",
    "create-stock-minimal": "node scripts/create-minimal-stock-data.js",
    "create-stock-enhanced": "node scripts/add-real-data-correct.js",
    "check-tables": "node scripts/check-products-structure.js",
    "verify-stock": "node scripts/check-stock-table-structure.js",
    "test-real-data": "node scripts/test-real-data-system.js",
    "fix-relationships": "node scripts/fix-stock-relationships.js",


    "db:seed": "npm run seed:simple",
    "db:check": "node scripts/check-database-status.js"
  },
